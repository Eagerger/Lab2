<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
 <package name="default" extends="struts-default">
        
        <action name="Search" class="action.BookAction"  method="searchA">
        <result>/title.jsp</result>
        </action>
         <action name="Delete" class="action.BookAction"  method="delete">
        <result >/delete.jsp</result>
        </action>
         <action name="Information" class="action.BookAction" method="showinformation" >
        <result>/information.jsp</result>
        </action>
        <action name="List" class="action.BookAction" method="showlist">
        <result>/list.jsp</result>
        </action>
      
    </package>
</struts>